
### {{ display_name }}
curl 'https://api.13012345822.com/api/{{ module_name }}.{{ class_name }}/create/' \
  -H 'Content-Type: application/json'\
  -d '{
    {% for property in properties -%}
    {% if property.is_parameter %}
    "{{ property.field_name }}":{{ property.example_value }},
    "--{{ property.field_comment or table_comment+property.field_name }}":"",
    {% endif %}
    {%- endfor %}
    "--":""
  }'

# 响应:
{
  "code": 1,
  "msg": "成功",
  "data": {
    {%- for property in properties %}
    "{{ property.field_name }}":{{ property.example_value }}{% if not loop.last or enum_fields|length >0 %},{% endif %} // {{ property.field_comment or table_comment+property.field_name }}
    {%- endfor %}
  }
}

