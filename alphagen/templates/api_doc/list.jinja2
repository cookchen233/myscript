### 文档区域（请勿修改）
### {{ display_name }}
curl 'https://localhost:9091/api/{{ module_name }}.{{ class_name }}/list/' \
  -H 'Content-Type: application/json'\
  -H 'site-id: 18' \
  -d '{
    "page":1,
    "page_size":20,
    {% for property in properties -%}
    {% if property.is_parameter %}
    "{{ property.field_name }}":"{{ property.example_value }}",
    "--{{ property.field_comment or table_comment+property.field_name }}":"",
    {% endif %}
    {%- endfor %}
    "--":""
    }'

# 返回:
{
  "code": 1,
  "msg": "SUCCESS",
  "rid": "4143fy3ry8w8ucpe",
  "data": {
    "lists": {
      "total": 16,
      "per_page": 10,
      "current_page": 1,
      "last_page": 2,
      "data": [
        {
            {%- for property in properties %}
            "{{ property.field_name }}": {% if property.property_type == "string" %}"{{ property.example_value }}"{% else %}{{ property.example_value }}{% endif %}, //{{ property.field_comment or table_comment+property.field_name }}
            {%- endfor %}
        }
      ]
    }
  },
  "ref": null
}

